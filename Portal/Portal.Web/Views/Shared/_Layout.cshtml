<!DOCTYPE html>
<html dir="rtl" lang="fa">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - یورولاو</title>
    <meta name="description" content="@ViewData["MetaDescription"]" />
    <link rel="apple-touch-icon" sizes="152x152" href="/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="/favicon-16x16.png">
    <link rel="manifest" href="/site.webmanifest">
    <meta name="msapplication-TileColor" content="#da532c">
    <meta name="theme-color" content="#ffffff">

    <environment include="Development">
        <link rel="stylesheet" href="~/lib/bootstrap/css/bootstrap-rtl.min.css" />
        <link href="~/lib/mdi/css/materialdesignicons.css" rel="stylesheet" />

        <link rel="stylesheet" href="/lib/owl/assets/owl.carousel.css">
        <link rel="stylesheet" href="/lib/owl/assets/owl.theme.default.css">
        <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    </environment>
    <environment exclude="Development">
        <link rel="stylesheet" href="~/bundle-rtl.min.css" asp-append-version="true" />
    </environment>
</head>
<body>

    <partial name="_Header" />

    @RenderBody()

    @*<partial name="_CookieConsentPartial" />*@

    <partial name="_Footer" />


    <environment include="Development">
        <script src="~/lib/jquery/jquery.js"></script>
        <script src="~/lib/owl/owl.carousel.js"></script>
        <script src="~/js/site.js" asp-append-version="true"></script>
    </environment>
    <environment exclude="Development">
        <script src="~/bundle.min.js" asp-append-version="true"></script>
    </environment>

    @RenderSection("Scripts", required: false)
</body>
</html>
